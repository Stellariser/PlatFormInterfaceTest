<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kaung.mapper.FrameProperityMapper">


    <resultMap id="FrameResultMap" type="com.kaung.pogo.FrameProperity">
        <id column="id" jdbcType="BIGINT"  property="id"/>
        <result column="dataset_id" jdbcType="BIGINT"  property="dataset_id"/>
        <result column="classification_id" jdbcType="BIGINT"  property="classification_id"/>
        <result column="scene_id" jdbcType="BIGINT"  property="scene_id"/>
        <result column="create_person" jdbcType="VARCHAR"  property="create_person"/>
        <result column="create_time" jdbcType="VARCHAR"  property="create_time"/>
        <result column="path" jdbcType="VARCHAR"  property="path"/>
        <result column="target_id" jdbcType="BIGINT"  property="target_id"/>
    </resultMap>


    <insert id="addFrameProperity" parameterType="com.kaung.pogo.FrameProperity">
        insert into t_frame_properity (dataset_id, classification_id, scene_id,create_person,create_time,path,target_id) values (#{dataset_id},#{classification_id},#{scene_id},#{create_person},#{create_time},#{path},#{target_id})
    </insert>

    <delete id="deleteFrameProperityById" parameterType="int">
        delete from t_frame_properity where id = #{id};
    </delete>

    <update id="updateFrameProperity" parameterType="com.kaung.pogo.FrameProperity">
        update t_frame_properity set dataset_id=#{dataset_id} ,classification_id=#{classification_id},scene_id=#{scene_id},create_person=#{create_person},create_time=#{create_time},path=#{path},target_id=#{target_id}  where id =#{id}
    </update>


    <select id="queryFrameProperityById" resultMap="FrameResultMap">
        select *from t_frame_properity where id=#{id}
    </select>

    <select id="queryAllFrameProperity" resultMap="FrameResultMap">
        select * from t_frame_properity
    </select>

    <select id="queryFrameProperityByName" resultMap="FrameResultMap">
        select * from t_frame_properity where dataset_id=#{dataset_id}
    </select>

    <select id="queryGlobalFrame" resultMap="FrameResultMap" parameterType="com.kaung.vo.ProperityQueryInfo">
        select * from t_frame_properity f where 1=1
        <if test="sceneList.length >0 and sceneList !=null ">
            and f.scene_id in
            <foreach collection="sceneList" index="index" item="item" open="(" separator="," close=")">
                ${item}
            </foreach>
        </if>
        <if test="classcificationList.length>0 and classcificationList !=null ">
            and f.classification_id in
            <foreach collection="classcificationList" index="index" item="item" open="(" separator="," close=")">
                ${item}
            </foreach>
        </if>
        <if test="datasetList.length>0 and datasetList !=null ">
            and f.dataset_id in
            <foreach collection="datasetList" index="index" item="item" open="(" separator="," close=")">
                ${item}
            </foreach>
        </if>
    </select>
</mapper>
