<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kaung.mapper.FrameToSceneMapper">

    <insert id="addFrameToScene" parameterType="com.kaung.pogo.FrameToScene">
        insert into t_frame_to_scene (scene_id_id, frame_id) values (#{scene_id_id},#{frame_id})
    </insert>

    <delete id="deleteFrameToSceneById" parameterType="int">
        delete from t_frame_to_scene where id = #{id};
    </delete>

    <update id="updateFrameToScene" parameterType="com.kaung.pogo.FrameToScene">
        update t_frame_to_scene set scene_id_id=#{scene_id_id} ,frame_id=#{frame_id} where id =#{id}
    </update>


    <select id="queryFrameToSceneById" resultType="com.kaung.pogo.FrameToScene">
        select *from t_frame_to_scene where id=#{id}
    </select>

    <select id="queryAllFrameToScene" resultType="com.kaung.pogo.FrameToScene">
        select * from t_frame_to_scene
    </select>

    <select id="queryFrameToSceneByName" resultType="com.kaung.pogo.FrameToScene">
        select * from t_frame_to_scene where scene_id_id=#{scene_id_id}
    </select>
    <select id="getLastInsert" resultType="int">
        select LAST_INSERT_ID()
    </select>
</mapper>
